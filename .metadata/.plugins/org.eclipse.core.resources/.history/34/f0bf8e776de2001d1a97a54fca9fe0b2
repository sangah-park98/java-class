package com.tjoeun.collectionTest;

import java.util.HashSet;

public class HashSetTest2 {

	public static void main(String[] args) {
		
		HashSet<String> hset = new HashSet<>();
		
		hset.add("홍길동");
		System.out.println(hset.size() + ": " + hset);
		hset.add("임꺽정");
		System.out.println(hset.size() + ": " + hset);
		hset.add("장길산");
		System.out.println(hset.size() + ": " + hset);
		hset.add("일지매");
		System.out.println(hset.size() + ": " + hset);
	}
	
}
