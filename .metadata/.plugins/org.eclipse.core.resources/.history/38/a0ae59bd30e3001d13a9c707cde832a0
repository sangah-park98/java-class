package com.tjoeun.ThreadTerminate;

public class ThreadTerminate extends Thread {

	private boolean flag = true;
	
	public ThreadTerminate() {
	}
	
	public ThreadTerminate(String name) {
		super(name);
	}
	
	
	
	@Override
	public void run() {
		
		while(flag) {

			
			
			flag = false;
			
		}
	}
	
	
	
}
