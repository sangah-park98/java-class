package com.tjoeun.deamonThreadTest;

public class GameThread {
	
	public static void main(String[] args) {
		
		
		
		
		
		for(int i=0; i<10; i++) {
			System.out.println("신나게 게임하는 중...");
			try {
				Thread.sleep(500);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
		
		
		
	}
}
