package com.tjoeun.syncronizedMethodTest;

public class Student2 extends Thread {

	public Student2() {	}

	public Student2(String name) {
		super(name);
	}
	@Override
	public void run() {
		
//		도서 대여
		try {
			String book = LibraryMain2.library.lendBook();
			if(book ==null) {
				return;
			}
			sleep(2500);
			LibraryMain2.library.returnBook(book);
		} catch (InterruptedException e) {
//			e.printStackTrace();
		} 
		
//		도서 반납
		
	}
	
	
}
